{
    "name": "NTU course checklist",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "ntu.png"
    }, 
    "content_scripts": [
        {
            "matches": ["https://reg.aca.ntu.edu.tw/*"],
            "js": [
                "scripts/extract.js",
                "scripts/courseHandler.js",
                "scripts/display.js",
                "scripts/content.js"],
            "css": ["styles/content.css"]
        }
    ], 
    "permissions": ["activeTab"], 
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    }
}